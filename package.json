{
  "name": "aea-base",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^5.0.8",
    "@mui/icons-material": "^5.14.7",
    "@mui/material": "^5.14.7",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/cordova": "^11.0.1",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.46",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "cordova-plugin-chooser": "^1.3.2",
    "cordova-plugin-file": "^7.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && npm run build-interface",
    "compile-types": "tsc windowCordova.ts windowEncryption.ts -jsx react-jsx --declaration true --emitDeclarationOnly true && npm run compile-types-move",
    "compile-types-move": "mv windowCordova.d.ts ./src/types && mv windowEncryption.d.ts ./src/types",
    "build-interface": "tsc windowCordova.ts windowEncryption.ts window.d.ts --target esnext --removeComments true -m commonjs -jsx react-jsx --skipLibCheck true && npm run build-interface-move",
    "build-interface-move": "mv windowCordova.js ./build && mv windowEncryption.js ./build",
    "test": "tsc -h"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "homepage": "./"
}
